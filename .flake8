[flake8]
max-line-length = 88
extend-ignore = E203, W503, E501, C901
exclude = 
    .git,
    __pycache__,
    build,
    dist,
    .venv,
    venv,
    *.egg-info,
    .pytest_cache,
    .mypy_cache
per-file-ignores =
    __init__.py:F401
    tests/*:S101
    main.py:E402
import-order-style = google
application-import-names = app