{
  "database": {
    "pool_size": 10,
    "timeout": 30
  },
  "ai_providers": {
    "default": "openai",
    "retry_attempts": 3,
    "timeout": 30
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "security": {
    "token_expiry_hours": 24,
    "max_login_attempts": 5
  }
}