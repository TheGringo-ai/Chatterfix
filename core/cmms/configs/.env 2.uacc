# Security Configuration
JWT_SECRET_KEY=your-jwt-secret-here
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your-admin-password-here

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=./logs/uacc.log
