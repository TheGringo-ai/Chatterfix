# Universal AI Command Center - All AI Providers Configuration
# Copy this to .env and add your actual API keys

# OpenAI (GPT-4, GPT-3.5, DALL-E, Whisper)
OPENAI_API_KEY=your_openai_key_here
OPENAI_ORG_ID=your_openai_org_id_here

# Anthropic (Claude 3.5 Sonnet, Claude 3 Haiku, Claude 3 Opus)
ANTHROPIC_API_KEY=your_anthropic_key_here

# Google AI (Gemini Pro, Gemini Ultra, PaLM)
GOOGLE_AI_API_KEY=your_google_ai_key_here
GOOGLE_PROJECT_ID=your_google_project_id_here

# xAI (Grok)
XAI_API_KEY=your_xai_key_here

# Hugging Face (Transformers, Inference API)
HUGGINGFACE_API_KEY=your_huggingface_key_here

# Ollama Local Models
OLLAMA_BASE_URL=http://localhost:11434

# Cloud Providers for Deployment
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_DEFAULT_REGION=us-east-1

GCP_PROJECT_ID=your_gcp_project_id_here
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/gcp-credentials.json

AZURE_CLIENT_ID=your_azure_client_id_here
AZURE_CLIENT_SECRET=your_azure_client_secret_here
AZURE_TENANT_ID=your_azure_tenant_id_here

# GitHub Integration
GITHUB_TOKEN=your_github_token_here

# Development Database
DATABASE_URL=postgresql://user:password@localhost:5432/uacc_dev

# Universal AI Command Center Settings
UACC_SECRET_KEY=your_secret_key_here
UACC_DEBUG=true
UACC_PORT=8889