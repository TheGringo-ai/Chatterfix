# ChatterFix Mobile Environment Configuration
# Copy this file to .env and fill in your values

# API Configuration
API_BASE_URL=https://chatterfix.com

# Firebase Configuration (get from Firebase Console)
FIREBASE_API_KEY=your-firebase-api-key
FIREBASE_AUTH_DOMAIN=fredfix.firebaseapp.com
FIREBASE_PROJECT_ID=fredfix
FIREBASE_STORAGE_BUCKET=fredfix.firebasestorage.app
FIREBASE_DATABASE_URL=https://fredfix-default-rtdb.firebaseio.com
FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
FIREBASE_APP_ID=your-app-id
FIREBASE_MEASUREMENT_ID=your-measurement-id
